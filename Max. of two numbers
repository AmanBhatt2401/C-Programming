#include<iostream>
using namespace std;

int max(int x,int y){  //function to check the max of two numbers
    if(x>y)
        return x;
    else
        return y;
    }

int main(){
    int a,b;
    cout<<"Enter a and b:-";
    cin>>a>>b;
    
    int m=max(a,b); //function call
    
    cout<<m<<endl;
    return 0;
}
